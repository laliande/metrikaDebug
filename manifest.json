{
    "manifest_version": 2,
    "name": "_ym_debug",
    "version": "1",
    "icons": {
        "128": "img/101.png"
    },
    "browser_action": {
        "default_title": "Metrika Debug",
        "default_icon": "img/101.png",
        "default_popup": "html/popup.html"
    },
    "permissions": [
        "cookies",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}